{
  "expo": {
    "name": "Calculadora Alcalinidad",
    "slug": "calculadora-alcalinidad",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "calculadora-alcalinidad",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "android": {
      "package": "com.davidcarrillo.calculadoraalcalinidad",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1e40af"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.davidcarrillo.calculadoraalcalinidad"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "60f9f589-b1f9-4c64-b2bc-0a75fd18fe73"
      }
    }
  }
}
